journal = {The European Physical Journal D},
month = apr,
number = {3},
pages = {327--334},
title = {{Formation of negative hydrogen ion in positronium-hydrogen collisions}},
url = {http://www.springerlink.com/index/10.1140/epjd/e2008-00058-6},
volume = {47},
year = {2008}
}
