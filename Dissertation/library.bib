journal = {Journal of Physics B: Atomic and Molecular Physics},
number = {22},
pages = {6105--6119},
title = {{The theoretical treatment of low-energy e+-H2 scattering using Kohn trial functions containing Hylleraas-type functions}},
url = {http://iopscience.iop.org/0022-3700/20/22/020},
volume = {20},
year = {1987}
}
@article{Brownstein1968,
abstract = {The Kohn variational method for calculating scattering parameters is formulated in an abstract vector space in terms of the extremization of a quadratic form whose coefficients represent matrix elements of H-E. This manifestly possesses certain symmetry and invariance properties not apparent in the usual formulation. Under certain conditions the method provides a bound for the exact answer if the number of variational parameters, N, is large enough. The extraneous singularities in the variationally determined value of tan$\delta$ noticed by Schwartz are analyzed. These are a direct consequence of the Kohn method and the particular normalization employed. It is shown that the pole strength (residue) of each such singularity is the product of two terms, each of which is small; these factors may be expected to decrease with increasing N.},
author = {Brownstein, Kenneth and McKinley, William},
doi = {10.1103/PhysRev.170.1255},
file = {:C$\backslash$:/Users/Denton/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Brownstein, McKinley - 1968 - Study of the Singularities in the Kohn Variational Method for Scattering Parameters.pdf:pdf},
