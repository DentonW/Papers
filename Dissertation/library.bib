file = {:C$\backslash$:/Users/Denton/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Andrade et al. - 2012 - Time-dependent density-functional theory in massively parallel computer architectures the OCTOPUS project.pdf:pdf},
issn = {1361-648X},
journal = {Journal of physics. Condensed matter : an Institute of Physics journal},
keywords = {Benzene,Benzene: chemistry,Computer Graphics,Computers,Models, Molecular,Molecular Conformation,Quantum Theory,Software,Time Factors},
month = jun,
number = {23},
pages = {233202},
pmid = {22562950},
title = {{Time-dependent density-functional theory in massively parallel computer architectures: the OCTOPUS project.}},
