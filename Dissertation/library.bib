title = {{Maple procedures for the coupling of angular momenta I. Data structures and numerical computations}},
url = {http://linkinghub.elsevier.com/retrieve/pii/S0010465509001969 http://linkinghub.elsevier.com/retrieve/pii/S0010465597000325},
volume = {103},
year = {1997}
}
